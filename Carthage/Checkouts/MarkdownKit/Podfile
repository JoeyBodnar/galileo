# Uncomment the next line to define a global platform for your project

target 'Example' do
  platform :ios, '10.0'
  use_frameworks!
  pod 'Alamofire', '~> 5.0.0.beta.1'
  pod 'MarkdownKit', :path => '.'
end

target 'Example-AppKit' do
  platform :osx, '10.12'
  use_frameworks!
  pod 'Alamofire', '~> 5.0.0.beta.1'
  pod 'MarkdownKit', :path => '.'
end
